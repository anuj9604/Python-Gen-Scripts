    "from sympy import nextprime\n",
    "def prime_factor(num):\n",
    "    p=2\n",
    "    prime_fact=[]\n",
    "    while num!=1:\n",
    "        if num%p==0:\n",
    "            prime_fact.append(p)\n",
    "            while num%p==0:\n",
    "                num=num/p\n",
    "        p=nextprime(p)\n",
    "    print(max(prime_fact))\n",
    "\n",
    "prime_factor(600851475143)"
   ]
  }
