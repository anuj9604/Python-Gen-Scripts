    "def count_vow(s):\n",
    "    vow={'a':1,'e':5,'i':9,'o':15,'u':21}\n",
    "    c_vow= 0\n",
    "    sum_vow=0\n",
    "    for _ in s:\n",
    "        if _ in vow:\n",
    "            c_vow+=1\n",
    "            sum_vow+=vow[_]\n",
    "    print(\"Number of vow are: {} and sum is {}\".format(c_vow, sum_vow))"
   ]
